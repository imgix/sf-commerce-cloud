<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="imgixApiKey">
                <display-name xml:lang="x-default">imgix API Key</display-name>
                <description xml:lang="x-default">The API Key of imgix account</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="imgixDefaultParams">
                <display-name xml:lang="x-default">imgix Page Designer Default Params</display-name>
                <description xml:lang="x-default">imgix Default Params to be set for images rendered using the Page Designer integration</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="imgixBaseURL">
                <display-name xml:lang="x-default">imgix Base URL</display-name>
                <description xml:lang="x-default">Include Protocol Eg: http:// or https://</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="imgixProductDefaultParams">
                <display-name xml:lang="x-default">imgix Product Default Params</display-name>
                <description xml:lang="x-default">imgix Default Params to be set for images rendered using the product integration</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="imgixAttributes">
                <display-name xml:lang="x-default">imgix Attributes</display-name>
                <attribute attribute-id="imgixApiKey"/>
                <attribute attribute-id="imgixDefaultParams"/>
                <attribute attribute-id="imgixBaseURL"/>
                <attribute attribute-id="imgixProductDefaultParams"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
    <type-extension type-id="Product">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="imgixData">
                <display-name xml:lang="x-default">Imgix Images</display-name>
                <type>text</type>
                <localizable-flag>false</localizable-flag>
                <site-specific-flag>false</site-specific-flag>
                <mandatory-flag>false</mandatory-flag>
                <visible-flag>false</visible-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <order-required-flag>false</order-required-flag>
                <externally-defined-flag>false</externally-defined-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="imgix">
                <display-name xml:lang="x-default">Imgix</display-name>
                <attribute attribute-id="imgixData"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
