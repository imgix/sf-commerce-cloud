{
  "name": "sf-commerce-cloud-fe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "imgix-management-js": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-imgix": "^9.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "scripts": {
    "start": "yarn start:int",
    "deploy": "yarn build:int && yarn build:bm",
    "start:int": "REACT_APP_APP_TYPE=int react-scripts start",
    "start:bm": "REACT_APP_APP_TYPE=bm react-scripts start",
    "build:int": "REACT_APP_APP_TYPE=int react-scripts build && cp -R build ../cartridges/int_imgix_pd/cartridge/experience/components/imgix/react_app",
    "build:bm": "REACT_APP_APP_TYPE=bm react-scripts build && cp -R build ../cartridges/bm_imgix_pd/cartridge/scripts/imgix/react_app",
    "test": "yarn test:all",
    "test:ci": "CI=true yarn test:int && CI=true yarn test:bm",
    "test:all": "yarn test:int && yarn test:bm",
    "test:int": "yarn build:int && REACT_APP_APP_TYPE=int react-scripts test -t='int_imgix_pd'",
    "test:bm": "yarn build:bm && REACT_APP_APP_TYPE=bm react-scripts test -t='bm_imgix_pd'",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@types/jsuri": "^1.3.30",
    "jsuri": "^1.3.1",
    "@types/react-imgix": "^9.0.4",
    "ts-jest": "^27.0.5"
  }
}
